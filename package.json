{
  "name": "vite-test-lab",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vue-tsc -b && vite build",
    "build:prod": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:snapshotUpdate": "vitest -u",
    "test:coverage": "vitest --coverage",
    "safe-build": "vitest run && npm run build",
    "start-back-end": "json-server --watch db.json --port 3000",
    "test:with-backend": "start-server-and-test start-back-end http://localhost:3000 test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KhaledObaidCubes/vite-test-lab.git"
  },
  "keywords": [
    "vitest",
    "vue",
    "vite"
  ],
  "author": "Khaled Obaid",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KhaledObaidCubes/vite-test-lab/issues"
  },
  "homepage": "https://github.com/KhaledObaidCubes/vite-test-lab#readme",
  "dependencies": {
    "axios": "^1.11.0",
    "uuid": "^11.1.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12",
    "json-server": "^1.0.0-beta.3",
    "start-server-and-test": "^2.0.8",
    "wait-on": "^7.2.0"
  }
}
